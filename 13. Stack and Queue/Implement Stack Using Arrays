class stack {
    int size = 10000;
    //Initialize an array to implement stack using arrays.
    int arr[] = new int[size];
    
    int top = -1;
    void push(int x) {
        top++;
        arr[top] = x;
    }
    int pop() {
        int x = arr[top];
        top--;
        return x;
    }
    int top() {
        return arr[top];
    }
    int size() {
        return top + 1;
    }
    boolean isEmpty(){
      return (top == -1);
}
